{
  "name": "leetcode",
  "version": "0.1.0",
  "private": true,
  "description": "The solutions for leetcode",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbwa/leetcode.git"
  },
  "keywords": [
    "leetcode"
  ],
  "author": "Bowen <asyncmode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lbwa/leetcode/issues"
  },
  "homepage": "https://github.com/lbwa/leetcode#readme",
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "\\\\node_modules\\\\"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "preset": "ts-jest",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\",
      "\\\\examples\\\\",
      "\\\\dist\\\\"
    ],
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1",
      "^data-structures/(.*)": "<rootDir>/data-structures/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
